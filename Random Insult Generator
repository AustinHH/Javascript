var randomBodyParts = ['head', 'feet', 'stomach', 'ears'];
var randomAdjectives = ['ugly', 'big', 'stinky', 'dumb'];
var randomWords = ['table', 'notebook', 'snail', 'can', 'phone'];

var randomBodyPart = randomBodyParts[
Math.floor(Math.random() * 4)
];

var randomAdjective = randomAdjectives[
Math.floor(Math.random() * 4)
];

var randomWord = randomWords[
Math.floor(Math.random() * 5)
];

console.log('Your ' + randomBodyPart + ' is like a '
 + randomAdjective + ' ' + randomWord
);
